		"format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
		"gen-api": "openapi --input ./src/api/swagger/openapi.json --output ./src/api/swagger --client axios"
	},
	"author": {
		"name": "John Smith",
		"email": "rintran720@gmail.com"
	},
	"dependencies": {
